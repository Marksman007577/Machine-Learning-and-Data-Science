    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "x = np.arange(0,10)\n",
    "y = 2*x\n",
    "\n",
    "plt.plot(x,y)\n",
    "plt.title(\"Linear Plot\")\n",
    "plt.xlabel(\"X-Axis\")\n",
    "plt.ylabel(\"Y-Axis\")\n",
    "plt.xlim(0,10)\n",
    "plt.ylim(0,19)\n",
    "plt.show()\n",
    "\n",
